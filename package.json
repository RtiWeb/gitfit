{
	"name": "gitfit",
	"version": "0.0.6",
	"description": "Git Feature Branch Tooling.",
	"homepage": "https://cjpatoilo.com/gitfit",
	"repository": "cjpatoilo/gitfit",
	"license": "MIT",
	"author": "CJ Patoilo <cjpatoilo@gmail.com>",
	"bin": "index.js",
	"keywords": [
		"git",
		"github",
		"opensource"
	],
	"ignore": [
		"artwork.psd",
		".appveyor.yml",
		".editorconfig",
		".github",
		".gitignore",
		".travis.yml"
	],
	"dependencies": {
		"git-branch": "^2.0.1",
		"rasper": "latest"
	},
	"devDependencies": {
		"ava": "^0.25.0",
		"boeing": "latest",
		"editorconfig-tools": "^0.1.1",
		"eslint": "^4.17.0",
		"eslint-config-styled": "0.0.2",
		"husky": "^0.14.3",
		"marshmallow": "latest",
		"nyc": "^11.4.1",
		"rimraf": "^2.6.2"
	},
	"engines": {
		"node": ">=6"
	},
	"scripts": {
		"prepush": "marshmallow -o docs -t 'Gitfit - Git Feature Branch Tooling.' -i artwork.psd -f -m && boeing docs && rimraf docs",
		"precommit": "npm t",
		"test": "nyc ava && eslint . -c styled --ignore-path .gitignore && editorconfig-tools check ."
	}
}
